{
  "name": "network-modification-tools",
  "version": "0.1.0",
  "private": true,
  "description": "网络修改工具,一、对请求的链接进行拦截、重定向；二、对请求的headers进行修改；三、对响应的headers进行修改。",
  "author": "whitesev",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-watch-prod": "vue-cli-service build --watch --mode production --target app --dashboard",
    "build-watch-dev": "vue-cli-service build-watch"
  },
  "dependencies": {
    "element-ui": "^2.3.6",
    "file-saver": "^2.0.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-cli-plugin-chrome-ext": "0.0.5",
    "vue-cli-plugin-element-ui": "~1.1.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
